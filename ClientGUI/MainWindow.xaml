<Window x:Class="ClientGUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ClientGUI"
        mc:Ignorable="d"
        Title="Velib" Height="400" Width="750">
    <Grid Margin="0,10,2,-5">
        <ComboBox x:Name="CityBox" HorizontalAlignment="Left" Margin="110,29,0,0" VerticalAlignment="Top" Width="191" SelectionChanged="CityBox_SelectionChanged"/>
        <ListBox x:Name="StationsList" HorizontalAlignment="Left" Height="239" Margin="382,67,0,0" VerticalAlignment="Top" Width="348" SelectionChanged="StationsList_SelectionChanged"/>
        <Label Content="Ville :" HorizontalAlignment="Left" Margin="10,29,0,0" VerticalAlignment="Top" Width="73"/>
        <TextBox x:Name="SearchStationTextBox" HorizontalAlignment="Left" Height="23" Margin="382,29,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="241" TextChanged="TextBox_TextChanged"/>
        <Label x:Name="AvailableBikesLabel" Content="" HorizontalAlignment="Left" Margin="382,311,0,0" VerticalAlignment="Top" Width="348" Height="43"/>

    </Grid>
</Window>
